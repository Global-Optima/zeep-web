# ==============================
# PostgreSQL Database Configuration
# ==============================
DB_USER=your_db_user         # Database username
DB_PASSWORD=your_db_password # Database password
DB_NAME=your_db_name         # Database name

# ==============================
# Redis Configuration
# ==============================
REDIS_PASSWORD=your_redis_password # Password for Redis
REDIS_DB=0                         # Default Redis database index

# ==============================
# Frontend Configuration
# ==============================
VITE_API_URL=http://backend:8080       # API URL for frontend (change for production)
VITE_WS_URL=ws://backend:8080/api/v1   # WebSocket URL for frontend

# ==============================
# pgAdmin Configuration (Optional for Development)
# ==============================
PG_EMAIL=your_pgadmin_email@example.com # Default pgAdmin login email
PG_PASSWORD=your_pgadmin_password       # Default pgAdmin password
